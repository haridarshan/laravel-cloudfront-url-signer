{
  "name": "haridarshan/laravel-cloudfront-url-signer",
  "description": "Wrapper around the official AWS PHP SDK to generate CloudFront signed URLs",
  "keywords": [
    "laravel-cloudfront-url-signer",
    "private content",
    "cloudfront",
    "aws",
    "aws-php-sdk",
    "aws-sdk-php-laravel",
    "signed-cookie",
    "signed-url"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Hari Darshan Gorana",
      "email": "er.haridarshan@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "aws/aws-sdk-php-laravel": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.6",
    "laravel/framework": "^9.0 || ^10.0"
  },
  "autoload": {
    "psr-4": {
      "Haridarshan\\Laravel\\CloudFrontUrlSigner\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aws\\Laravel\\AwsServiceProvider",
        "Haridarshan\\Laravel\\CloudFrontUrlSigner\\CloudFrontUrlSignerServiceProvider"
      ],
      "aliases": {
        "AWS": "Aws\\Laravel\\AwsFacade",
        "CloudFrontUrlSigner": "Haridarshan\\Laravel\\CloudFrontUrlSigner\\Facades\\CloudFrontUrlSignerFacade"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
